{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    {
      "identifier": "fs:default",
      "allow": [
        "**"
      ]
    },
    {
      "identifier": "http:default",
      "allow": [
        "http://*:*",
        "https://*:*"
      ]
    },
    "store:default",
    "log:default"
  ]
}